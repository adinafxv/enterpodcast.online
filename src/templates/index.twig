{% extends '_layouts/base.twig' %}

{% block title %}Enter podcast{% endblock %}

{% block bodyClass %}{% endblock %}

{% block header %}{% endblock %}

{% block content %}
<div class="container bg">
  <aside>
    <div class="aside-flex">
      <div class="top">
       <div class="logo" aria-label="logo consisting of radio waves and a word 'enter'">
                {% include '_partials/_svg/_logo.svg' %} 
        </div>
      </div>
       

        <div class="contacts">
          <div class="contacts-item social">
            <strong>Odkazy</strong></strong>
              {% include '_partials/_share.twig' %}
          </div>

          <div class="contacts-item place">
            <strong>Lokace</strong><!-- sídlo-->
            <p>Praha, Česká republika</p>
          </div>

         

        </div>
    </div>
    
  </aside>


  <main role="main">
    <div class="text">
      <h1>Enter podcast</h1>
      <p class="bio">Podcast o&nbsp;nových začátcích a kódování. Už brzy online.</p>
      <iframe src="https://player.rss.com/enter-po/351550?theme=light" style="width: 100%" title="Enter podcast" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen>
      <a href="https://rss.com/podcasts/enter-po/351550/">#0 Pilot | RSS.com Podcasting</a></iframe>
    </div>
  </main>
</div>
  <footer role="contentinfo">
  <div class="footer-container">
    <span>Adina Foxova &copy;2021, </span>
    <a class="footer-link" href="https://simpleanalytics.com/enterpodcast.online" target="_blank" rel="noopener">Analytics</a>
    <span>, všechna práva vyhrazena</span>
  </div>
  </footer>
{% endblock %}